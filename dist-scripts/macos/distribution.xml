<?xml version="1.0" encoding="utf-8" standalone="no"?>
<installer-script minSpecVersion="1.000000">
    <title>massdash 0.0.8</title>
    <background mime-type="image/png" file="MassDash_Logo_Dark.png" scaling="proportional"/>
    <welcome file="welcome.html" mime-type="text/html" />
    <conclusion file="conclusion.html" mime-type="text/html" />
    <license file="LICENSE.txt"/>
    <options customize="never" allow-external-scripts="no"/>
    <domains enable_localSystem="true" />
    <choices-outline>
        <line choice="massdash"/>
    </choices-outline>
    <choice id="massdash" title="massdash">
        <pkg-ref id="massdash.pkg"/>
    </choice>
    <pkg-ref id="massdash.pkg" auth="Root">massdash.pkg</pkg-ref>
</installer-script>